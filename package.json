{
  "name": "nc-file-merger",
  "version": "1.0.0",
  "description": "NC file merger tool with tool number remapping and preview",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:client",
    "dev:client": "vite",
    "dev:worker": "wrangler dev --local",
    "clean": "node -e \"fs=require('fs');fs.rmSync('dist',{recursive:true,force:true});\"",
    "build": "npm run clean && npm run build:client",
    "build:client": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy"
  },
  "keywords": [
    "hono",
    "nc",
    "gcode",
    "cnc",
    "merger"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.2025.1",
    "@tailwindcss/postcss": "^4.1.15",
    "@types/node": "^20.10.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.15",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^7.1.11",
    "wrangler": "^3.90.0"
  }
}
